<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Records Transfer management system</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body><header class="navbar navbar-inverse navbar-fixed-top">
    
        <div class="container">
            <div class="navbar-header">

                <div class="top-left" style="font-size:x-large; color:lightslategrey">
                    <img src="~/UNRAlogo.png" alt="unralogo" width="45px" length="15px"> Records Transfer Management System
                </div>
            </div>
          </div>
</header>  
  <div id="sidebar">
     @RenderSection("SideBar", required:false) 
  
  </div>
    <div  class="container body-content ">
        <hr />
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year Records Transfer management system</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>
